{
  "compilerOptions": {
    "module": "commonjs", // Keep as commonjs for Jest compatibility
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "moduleResolution": "node"
  }
}
